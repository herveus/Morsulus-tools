# Common client function to replace escaped URL characters with text.
sub decode {
  local($in) = @_;
  local($out);
  
  $in =~ s/\+/\ /g;
  $out = '';
  while ($in =~ /\%/) {
    $in =~ /^([^%]*)\%([0-9A-Fa-f][0-9A-Fa-f])(.*)$/;
    $out .= $1 . pack ('c', hex $2);
    $in = $3;
  }
  return $out . $in;
}

