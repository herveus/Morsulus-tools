# Common client function to read response from database server.
sub get_matches {
  #global (*S, @matches, @messages, @lost, %m);
  local ($n, $_);

  $n = 0;
  while (<S>) {
    chop;
    if (/^\'(.*)$/) {
      push (@messages, $1);
    } elsif (/^\d+\+(\d+)$/) {
      push (@lost, $_);
      $n += $1;
    } elsif (/^(\d+)\|/) {
      push (@matches, $_);
      $n++;
      $m{$1}++;
    }
  }
  close S;
  return $n;
}
  
